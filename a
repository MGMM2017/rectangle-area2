# rectangle_area.py (Updated)

def calculate_area(length, width):
    """
    Calculate the area of a rectangle.
    Raises a ValueError if length or width is negative.
    :param length: Length of the rectangle
    :param width: Width of the rectangle
    :return: Area of the rectangle
    """
    if length < 0 or width < 0:
        raise ValueError("Length and width must be non-negative")
    return length * width


def test_calculate_area():
    """
    Test case for calculate_area function.
    """
    assert calculate_area(5, 10) == 50, "Test Case 1 Failed"
    assert calculate_area(7, 3) == 21, "Test Case 2 Failed"
    assert calculate_area(0, 10) == 0, "Test Case 3 Failed"
    
    # Test for ValueError
    try:
        calculate_area(-5, 10)
    except ValueError:
        print("Caught expected ValueError for negative dimensions")
    
    print("All test cases passed!")


if __name__ == "__main__":
    test_calculate_area()
